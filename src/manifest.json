{
  "name": "马褂",
  "version": "2.0.0",
  "description": "多账号切换工具",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "js": ["contentscript.js"],
    "matches": ["*://*/*"]
  }],
  "browser_action": {
    "default_title": "",
    "default_icon": "asset/majia-128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "tabs",
    "*://*/*",
    "storage"
  ],
  "icons": {
    "16": "asset/majia-128.png",
    "48": "asset/majia-128.png",
    "128": "asset/majia-128.png"
  },
  "manifest_version": 2
}